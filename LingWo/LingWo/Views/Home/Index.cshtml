@using LingWo.Utility
@model IEnumerable<LingWo.Models.Video>
@{
    ViewData["Title"] = "Home Page";
}
@if (User.IsInRole(WC.AdminRole))
{
    <!-- Content Header (Page header) -->
    <div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">Ինֆորմացիոն վահանակ</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Գլխավոր</a></li>
                    <li class="breadcrumb-item active">Ինֆորմացիոն վահանակ</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
    <!-- /.content-header -->


    <!-- Small boxes (Stat box) -->
    <div class="container-fluid">
    <div class="row">
        <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-info">
                <div class="inner">
                    <h3>@ViewData["ArticlesCount"]</h3>

                    <p>Նորություններ</p>
                </div>
                <div class="icon">
                    <i class="ion ion-bag"></i>
                </div>
                <a asp-controller="News" asp-action="Index" class="small-box-footer">Ավելին <i
                        class="fas fa-arrow-circle-right"></i></a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-success">
                <div class="inner">
                    <h3>@ViewData["BlogsCount"]</h3>

                    <p>Բլոգ</p>
                </div>
                <div class="icon">
                    <i class="ion ion-stats-bars"></i>
                </div>
                <a asp-controller="Blog" asp-action="Index" class="small-box-footer">Ավելին <i
                        class="fas fa-arrow-circle-right"></i></a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-warning">
                <div class="inner">
                    <h3>@ViewData["UsersCount"]</h3>

                    <p>Հաշիվներ</p>
                </div>
                <div class="icon">
                    <i class="ion ion-person-add"></i>
                </div>
                <a asp-controller="User" asp-action="Index" class="small-box-footer">Ավելին<i
                        class="fas fa-arrow-circle-right"></i></a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-danger">
                <div class="inner">
                    <h3>@ViewData["CommentsCount"]</h3>

                    <p>Մեկնաբանություններ</p>
                </div>
                <div class="icon">
                    <i class="ion ion-pie-graph"></i>
                </div>
                <a asp-controller="Comment" asp-action="Index" class="small-box-footer">Ավելին <i
                        class="fas fa-arrow-circle-right"></i></a>
            </div>
        </div>
        <!-- ./col -->
    </div>
</div>
}
else
{
    <section class="main-section">
    <div class="main-image">
        <div class="container-custom ">
            <div class="main-content">
                <h1 id="main-text">Բարի գալուստ «LingWo» լեզուների առցանց դպրոց</h1>
                <h4 id="secondary-text">Մենք ձեզ համար կընտրենք ուսուցման անհատական ​​մեթոդ և նյութեր: «LingWo»
                    դպրոցում,
                    դուք կծանոթանաք ոչ միայն լեզվին, այլև մշակույթին, գրականությանը,
                    պատմություն և արվեստին</h4>
                <h5 id="select-language">Ընտրեք լեզու սովորելու համար</h5>

                <div class="get-started">
                    <div class="dropdown languages-selector">
                        <button class="btn dropdown-toggle language-btn" type="button" data-bs-toggle="dropdown"
                            aria-expanded="false">
                            <img src="~/img/main/England.svg" alt="">
                            Անգլերեն
                        </button>
                        <ul class="dropdown-menu">
                            <li>
                                <a class="dropdown-item" href="#">
                                    <img src="~/img/main/Japanese.svg" alt="">
                                    Ճապոներեն
                                </a>
                            </li>
                        </ul>
                    </div>
                    <button type="button" class="btn-main">Սկսել</button>
                </div>
            </div>
        </div>
    </div>
</section>

    <section class="about-section">
    <div class="container-custom ">
        <div class="about">
            <h2 class="block-title about-classes">Դասերը «Lingwo»-ում:</h2>
            <div class="info-block">
                <div class="info-small">
                    <img class="info-image" src="~/img/about/Child.jpg" alt="">
                    <h4 class="info-title">Արդյունավետ առցանց դասեր</h4>
                    <h5 class="info-text">Առցանց դասեր Google Meet-ում, 60 րոպե տևողությամբ։
                        Դասերի օրերը և դասընթացի տևողությունը համաձայնեցված են
                        ուսուցչի և աշակերտի միջև.</h5>
                </div>
                <div class="info-small">
                    <img class="info-image" src="~/img/about/OnlineMeeting.jpg" alt="">
                    <h4 class="info-title">Խմբակային կամ մասնավոր պարապմունքներ</h4>
                    <h5 class="info-text">Սովորեք մինչև 5 ուսանողից բաղկացած փոքր խմբերում կամ
                        մասնավոր պարապմունքներով լրացուցիչ ինտենսիվության համար:</h5>
                </div>
                <div class="info-small">
                    <img class="info-image" src="~/img/about/Girl.jpg" alt="">
                    <h4 class="info-title">Սկսնակից մինչև առաջադեմ</h4>
                    <h5 class="info-text">Ընտրեք այն մակարդակը, որը լավագույնս համապատասխանում է ձեզ: Մեր որակավորված ուսուցիչները կստեղծեն
                        ընկերական մթնոլորտ ձեզ համար, որն կօգնի ձեզ կատարելապես տիրապետել լեզվին:</h5>
                </div>
                <div class="info-small">
                    <img class="info-image" src="~/img/about/Man.jpg" alt="">
                    <h4 class="info-title">Դասերի ճկուն գրաֆիկ</h4>
                    <h5 class="info-text">Դպրոցն ապահովում է դասերի ճկուն գրաֆիկ:
                        Անկախ նրանից՝ դուք դպրոցում եք, թե աշխատում եք, մեր ուսուցիչները ձեր դասաժամերը կհամապատասխանեցնեն ձեր
                        ժամանակացույցին։</h5>
                </div>
            </div>
        </div>
    </div>
</section>

