    <section class="videos-section">
    <div class="container-custom">
        <h2 class="block-title">Տեսանյութեր լեզվի մասին</h2>
        <div class="videos-block">
            @foreach(var video in Model.Take(4))
            {
                <a asp-action="Watch" asp-controller="Video" asp-route-Id="@video.Id" class="video-link">
                    <img src="~/img/about-language/PlayButton.svg" alt="" class="btn-play">
                    <div class="video-content">
                        <h4 class="video-title">@video.Title</h4>
                    </div>
                </a>
            }
        </div>
        <div class="slider-points d-flex justify-content-center">
            <a href="" class="slider-point me-3 active-point"></a>
            <a href="" class="slider-point me-3"></a>
            <a href="" class="slider-point"></a>
        </div>
    </div>
</section>

    <section class="news-section">
    <div class="container-custom">
        <h2 class="block-title">Նորություններ</h2>
        <h4 class="news-about">Լեզվական ծրագրերի և միջազգային մրցույթների մասին տեղեկատվական տախտակ</h4>
        <div class="articles">
            <div class="article">
                <img class="article-logo" src="~/img/news/mextLogo.png" alt="Mext">
                <div class="article-content">
                    <h6 class="article-date">
                        2023/05/26
                    </h6>
                    <h5 class="article-title">
                        MEXT - Ճապոնիա
                    </h5>
                    <h4 class="article-info">
                        Ճապոնիայի կրթության, մշակույթի, սպորտի, գիտության և տեխնոլոգիաների նախարարությունը հայտարարում է
                        օտարերկրյա ուսանողների հավաքագրում, ովքեր ցանկանում են սովորել Ճապոնիայում՝ Ճապոնիայի կառավարություն
